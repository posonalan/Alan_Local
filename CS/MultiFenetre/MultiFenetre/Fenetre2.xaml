<Window x:Class="MultiFenetre.Fenetre2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiFenetre"
        mc:Ignorable="d"
        Title="Fenetre2" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="109*"/>
            <RowDefinition Height="65*"/>
            <RowDefinition Height="90*"/>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="120*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition Width="51*"/>
            <ColumnDefinition Width="63*"/>
        </Grid.ColumnDefinitions>

        <Label x:Name="lblMot" Content="Fenetre 2"  FontSize="25" Grid.Column="1" Grid.Row="1"/>
        <Button x:Name="btnRetour" Grid.Column="1" Grid.Row="3" Click="btnRetour_Click" Content="Retour" />
     
    </Grid>
</Window>
